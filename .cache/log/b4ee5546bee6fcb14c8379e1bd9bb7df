;(function($){
   	var platform = {},
   		ua = window.navigator.userAgent,
   		uaReg = {
   			'weixin': /MicroMessenger\/([0-9\.]+)/,
   			'didi_passenger': /didi\.passenger\/([0-9\.]+)/, 
   			'daijia_driver': /daijia\.driver\/([0-9\.]+)/,
   			'alipay': /AlipayClient\/([0-9\.]+)/,
   			'kd_passenger': /kd\.passenger\/([0-9\.]+)/,
   			'kd_driver': /kd\.driver\/([0-9\.]+)/,
    		'qq': /QQ\/([0-9\.]+)/,
  			'weibo': /weibo\_\_([0-9\.]+)_/
   		};

	$.each(uaReg, function(key, reg){
		var p = ua.match(reg);
		if(p){
			platform[key] = true;
			platform.info = p[0];
			platform.version = p[1];
			return false;
		}
	});

  	$.platform = platform;
    if(!window.KUI){
    	window.KUI = {};
    }
  	KUI.platform = platform;

})(Zepto);/* KOP - 接口平台接入 */
