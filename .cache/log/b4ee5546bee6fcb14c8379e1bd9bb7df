;(function($){

	var WXSDK = function(config){
		var _self = this;
		_self.config = $.extend({}, WXSDK.config, config);
		_self._init();
	};
	
	WXSDK.config = {
		kopConfig: null,
		debug:false,
		appId: 'wx0561c93f0b3fc4f0',
		jsApiList: [],
		initFunc: function(){}
	};
	
	$.extend(WXSDK.prototype, {	
		_init: function(){
			var _self = this,
				kop = new KUI.KOP(_self.config.kopConfig);
				
			_self.kop = kop;
			
			// 只在微信中生效
			if(window.navigator.userAgent.indexOf('MicroMessenger') === -1){
				return false;
			}
			
			kop.send('lj.u.p.getH5WeiXinSign', {
				refererUrl: window.location.href
			}, null, null, function(data){		
				wx.config({
					debug: _self.config.debug,
					appId: _self.config.appId,
					timestamp: data.timestamp,
					nonceStr: data.uuid,
					signature: data.sign,
					jsApiList: _self.config.jsApiList
				});
				wx.ready(_self.config.initFunc);
			
			}, function(err, msg){
				//console.log(err);
			});
			
		}

	});	
    
    if(!window.KUI){
    	window.KUI = {};
    }
    KUI.WXSDK = WXSDK;

})(Zepto);
/* 微信JS 分享 */
;(function($){

	var WXShare = function(config){
		var _self = this;
		_self.config = $.extend({}, WXShare.config, (window.WXShareConfig || {}), config);
		_self._init();
	};
	
	WXShare.config = {
		kopConfig: null,
		debug: false,
		title: '',
		desc: '',
		link: '',
		imgUrl: '',
		/*
		qqCfg: {},
		timelineCfg: {},
		friendCfg: {},
		weiboCfg: {},
		*/
		success: function(){},
		cancel: function(){}
	};
	
	$.extend(WXShare.prototype, {	
		_init: function(){
			var _self = this,
				sdk = new KUI.WXSDK({
					kopConfig: _self.config.kopConfig,
					debug: _self.config.debug,
					jsApiList: [
						'onMenuShareTimeline',
						'onMenuShareAppMessage',
						'onMenuShareQQ',
						'onMenuShareWeibo'
					],
					initFunc: _self._initShare()
				});
				
			_self.sdk = sdk;
					
		},
		resetShare: function(cfg){
			var _self = this;
			_self._initShare()(cfg);
		},
		_initShare: function(){
			var _self = this;
			return function(cfg){
				var	shareConfig = {
						title: _self.config.title,
						link: _self.config.link,
						desc: _self.config.desc,
						imgUrl: _self.config.imgUrl,
						success: _self.config.success,
						cancel: _self.config.cancel
					};
				wx.onMenuShareTimeline(_self._shareCfg(shareConfig, _self.config.timelineCfg, cfg));	
				wx.onMenuShareAppMessage(_self._shareCfg(shareConfig, _self.config.friendCfg, cfg));	
				wx.onMenuShareQQ(_self._shareCfg(shareConfig, _self.config.qqCfg, cfg));	
				wx.onMenuShareWeibo(_self._shareCfg(shareConfig, _self.config.weiboCfg, cfg));	
			}
		},
		_shareCfg:function(c1, c2, c3){
			var _self = this,
				cfg = $.extend({}, c1, c2 || null, c3 || null);
			cfg.link = _self._httpLink(cfg.link);
			cfg.imgUrl = _self._httpLink(cfg.imgUrl);
			return cfg;
		},
		_httpLink: function(link){
			var _self = this;
			if(/^\/\//.test(link)){
				var protocol = window.location.protocol.indexOf('https') > -1 ? 'https:' : 'http:'
				return protocol + link;
			}else{
				return link;
			}
		}
		
	});
    
    if(!window.KUI){
    	window.KUI = {};
    }
    KUI.WXShare = WXShare;

})(Zepto);
